{
  "expo": {
    "name": "GereuOnlineHub",
    "slug": "GereuOnlineHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "gereuonlinehub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onlinegereu.hub",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPushNotificationUsageDescription": "This app needs to send you notifications for new messages and updates."
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#00B2FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.onlinegereu.hub",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "notification": {
        "icon": "./assets/images/icon.png",
        "color": "#cb044d"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#00B2FF"
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#cb044d"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "232136a8-7373-4fda-aaa8-966ccccd1bc2"
      }
    }
  }
}
